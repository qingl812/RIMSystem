<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.rimsystem.mapper.RoadDocMapper">
    <update id="updateDocPath">
        update road_doc set doc_path=#{param1} where id=#{param2}
    </update>

    <select id="selectAllRoadDocType" resultType="java.lang.String">
        select type_name from road_doc_type
    </select>
</mapper>